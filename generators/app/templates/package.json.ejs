{
    "name": "<%= props.appName %>",
    "description": "<%= props.appDesc %>",
    "version": "0.0.0",
    <% if (props.bundler !== 'None') { %>
    "scripts": {
      <% if(props.bundler === 'Webpack') { %>
      "start": "webpack-dev-server -d",
      "build": "webpack"
      <% } else if(props.bundler === 'Parcel') { %>
      "start": "parcel index.html",
      "build": "parcel build index.html"
      <% } %>
    },
    "devDependencies": {
      <% if(props.bundler === 'Webpack') { %>
      "@wasm-tool/wasm-pack-plugin": "0.2.3",
      "html-webpack-plugin": "^3.2.0",
      "webpack": "^4.29.5",
      "webpack-cli": "^3.2.3",
      "webpack-dev-server": "^3.1.14"
      <% } else if(props.bundler === 'Parcel') { %>
      "parcel-bundler": "^1.11.0",
      "parcel-plugin-wasm.rs": "^1.2.8"
      <% } %>
    }
    <% } %>
  }
  